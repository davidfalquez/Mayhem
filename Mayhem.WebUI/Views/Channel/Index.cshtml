@model IEnumerable<Mayhem.DTO.Channel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            ChannelName
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ChannelName)
        </td>
        <td>
            @Html.ActionLink("[Edit]", "Edit", new { channelId = item.ChannelId }) |
            @Html.ActionLink("[Delete]", "Delete", new { channelId = item.ChannelId })
        </td>
    </tr>
}

</table>
