@model IEnumerable<Mayhem.DTO.Dispatcher>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
    <th>
            Dispatcher ID
        </th>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Role Type ID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DispatcherId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RoleTypeId)
        </td>
        <td>
            @Html.ActionLink("[Edit]", "Edit", new { dispatcherId = item.DispatcherId }) |
            @Html.ActionLink("[Delete]", "Delete", new { dispatcherId = item.DispatcherId })
        </td>
    </tr>
}

</table>
